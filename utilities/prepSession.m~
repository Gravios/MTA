function prepSession(Session)

%slist = {'jg04-20120128','jg04-20120130','jg05-20120309','jg05-20120310','jg05-20120315'};
%slist = {'er01-20110721','jg04-20120210','jg04-20120211','jg04-20120212','jg04-20120213'};

%Session = MTASession('er01-20110721');

Session = 'jg04-20120128';

Session = MTASession(Session)
Session = Session.autoLabelBhv;
Session.save;



plot(Session.xyz(:,1,3))

Trial = MTATrial(Session,{},[],'default_height_restricted',1);
Trial.save;

plot(Trial.xyzPeriods(1):Trial.xyzPeriods(end),Trial.xyz(:,1,3),'r')


MTAPlaceField(Trial,[],{'walk','theta'},1)
MTAPlaceField(Trial,[],{'rear','theta'},1)
MTAPlaceField(Trial,[],{'head','theta'},1)
MTAPlaceField(Trial,[],{'nrhp','theta'},1)

MTAPlaceField(Trial,[],{'walk'},1)
MTAPlaceField(Trial,[],{'rear'},1)
MTAPlaceField(Trial,[],{'head'},1) 
MTAPlaceField(Trial,[],{'nrhp'},1)
